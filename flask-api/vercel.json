{
  "version": 2,
  "builds": [
    {
      "src": "flask-api/api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "nextjs-app/*",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/flask-api/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/nextjs-app/$1"
    }
  ]
}
